<h1>books</h1>
<%= @books,each do |book| %>

  <p>title</p>
  <span><%= book.title %></span>
<% end %>

<h3>New Books</h3>

<%= form_with model: @book, url:'/books', method: :post do |f| %>

  <h4>title</h4>
  <%= f.text_field :title %>

  <h4>body</h4>
  <%= f.text_area :boby %>

  <%= f.submit'Create Book' %>
<% end %>
